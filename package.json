    "prettier": "^3.6.2",
    "sass": "^1.93.2",
    "sass-loader": "^16.0.5",
    "style-loader": "^3.3.0",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.4.0",
    "typescript": "^5.1.0",
